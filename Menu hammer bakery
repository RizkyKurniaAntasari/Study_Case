#include <iostream>
using namespace std;

int main()
{
    cout << "+------------------------------------------------------------+" << endl;
    cout << "| _  _                            ___       _                |" << endl;
    cout << "|| || |__ _ _ __  _ __  ___ _ _  | _ ) __ _| |_____ _ _ _  _ |" << endl;
    cout << "|| __ / _` | '  \\| '  \\/ -_) '_| | _ \\/ _` | / / -_) '_| || ||" << endl;
    cout << "||_||_\\__,_|_|_|_|_|_|_\\___|_|   |___/\\__,_|_\\_\\___|_|  \\_, ||" << endl;
    cout << "|                  |  \\/  |___ _ _ _  _                 |__/ |" << endl;
    cout << "|                  | |\\/| / -_) ' \\ || |                     |" << endl;
    cout << "|                  |_|  |_\\___|_||_\\_,_|                     |" << endl;
    cout << "+------------------------------------------------------------+" << endl;
    cout << "|                                                            |" << endl;
    cout << "|    ChesseCake                                              |" << endl;
    cout << "|                                                            |" << endl;
    cout << "|       # Burnt ChesseCake   (slice / whole)      20k/65k    |" << endl;
    cout << "|       # Lotus ChesseCake   (Slice / Whole)      16k/55k    |" << endl;
    cout << "|                                                            |" << endl;
    cout << "|    Brownies                                                |" << endl;
    cout << "|                                                            |" << endl;
    cout << "|      # Brownies Bites  (8pcs / 16pcs)           35k/55k    |" << endl;
    cout << "|      # Fudgy Brownies  (whole)                  60k        |" << endl;
    cout << "|                                                            |" << endl;
    cout << "|                                        (                   |" << endl;
    cout << "|                                         )                  |" << endl;
    cout << "|                                     _..--.._               |" << endl;
    cout << "|                                  -=' / | \\ `=-.            |" << endl;
    cout << "|                                 :--.._____..--;            |" << endl;
    cout << "|                                  \\.,_______,./             |" << endl;
    cout << "+------------------------------------------------------------+" << endl;

    return 0;
}
